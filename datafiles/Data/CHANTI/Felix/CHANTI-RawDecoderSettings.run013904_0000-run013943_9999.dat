# Total number of channels of the readout board (used by RecoBase/src/FelixRawDecoder.cc)
NChannels= 768

# Total number of ROBoards
NROBoards= 2

# Number of optical links for a full Felix server
NROMezzaninesPerFullBoard= 12

# Number of readout boards for each station [one value for each station]
NROBoardsPerStation= 2

# Optical link masks to indicate the enabled links in each server [one value for each ROBoard]
ROMezzanineMasksPerBoard= fff fff

# Number of 25ns-long slots read [one value for each mezzanine]
NSlots= 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8 8

# Index of last 25ns-long slot [one value for each mezzanine]
LastSlotID= 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4 4

# Offset used for matching trigger in the Felix soft
Latency= -288

# Hit structure version to use
HitVersion= 2

# Flag to define the treatment of warning printouts
#0: no warnings
#1: general information event-related
#2: general information detector-related
#3: maximum detail level
WarningsLevel= 3

# Total number of channels of the readout board (used by RecoBase/src/FelixRawDecoder.cc)
NROChannels= 768

