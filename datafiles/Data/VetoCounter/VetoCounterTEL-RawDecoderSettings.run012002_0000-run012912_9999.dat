# Total number of channels
NChannels= 512

# Total number of ROBoards
NROBoards= 1

# Number of mezzanines for a full board
NROMezzaninesPerFullBoard= 4

# Number of readout mezzanines for each station [one value for each station]
NROBoardsPerStation= 1

# Mezzanine masks to indicate the enabled mezzanines in each board [one value for each ROBoard]
#ROMezzanineMasksPerBoard= e
ROMezzanineMasksPerBoard= 4

# FPGA map: number of FPGA per readout (DAQ) board (i.e number of TDCs per Tel62)
# "NFPGAs" has to be specified for all RO boards used
NFPGAs= 4

# Number of 25ns-long slots read [one value for each mezzanine]
NSlots= 8 8 8 8

# Index of last 25ns-long slot [one value for each mezzanine]
